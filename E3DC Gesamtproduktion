var Wert1, Wert2, tempValue;


on({id: new RegExp('javascript\\.0\\.e3dc\\.modbus\\.externeQuelle' + "|" + 'modbus\\.0\\.holdingRegisters\\.40068_PV_Leistung'), change: "ne"}, function (obj) {
    Wert1 = getState("javascript.0.e3dc.modbus.externeQuelle").val;
  Wert2 = getState("modbus.0.holdingRegisters.40068_PV_Leistung").val;
  tempValue = Wert1 + Wert2;
  setState("0_userdata.0.TestE3DC"/*E3DC-Gesamt-Produktion*/, tempValue, true);
});


